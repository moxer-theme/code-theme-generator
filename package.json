{
  "name": "@moxer/vscode-theme-generator",
  "version": "1.0.0",
  "description": "Easily generate themes for VS Code",
  "main": "dist/index.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "prestart": "tsc demo/generate.ts",
    "start": "node demo/generate.js",
    "prepublish": "npm run build",
    "lint": "tslint src/*.ts"
  },
  "author": "Mattia Astorino",
  "license": "MIT",
  "typescript": {
    "definition": "index.d.ts"
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "typings": "dist/index",
  "devDependencies": {
    "@types/node": "^6.14.6",
    "tslint": "^5.9.1",
    "tslint-consistent-codestyle": "^1.13.0",
    "typescript": "^2.8.3"
  }
}
