{
  "javascript.validate.enable": false,
  "files.exclude": {
    "**/.git": true,
    ".git-crypt": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.js.map": true
  },
  "editor.formatOnSave": false,
  "markdownlint.config": {
    "MD033": false
  },
  "eslint.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.codeActionsOnSaveTimeout": 2000
}
